<!--=== Popper Min Js ===-->
<script src="_assets/js/vendor/popper.min.js"></script>
<!--=== Bootstrap Min Js ===-->
<script src="_assets/js/vendor/bootstrap.min.js"></script>
<!--=== Plugins Min Js ===-->
<script src="_assets/js/plugins.js"></script>

<!--=== Active Js ===-->

</body>

</html>


<script>
 function funCha(){
  var x = document.getElementById("dmcha").value; 
  var link="getdmcons/"+x;
  //alert(link)
  $('#dmCon').load(link); 
 }
 
 //load
 $('#dmcha').load('getdmchas'); 
 $('#tinh').load('gettinhs'); 

//function quân huyện
 function funHuyen(){
  var x = document.getElementById("tinh").value;
  var link="getqhs/"+x;
  $('#huyen').load(link); 
 }
 function funXa(){
  var x = document.getElementById("huyen").value; 
  var link="getphuongxas/"+x;
  $('#xa').load(link); 
 }

</script>

<style>
select#dmcha ,select#dmCon,select#tinh,select#huyen,select#xa,select#country,input#yes{
    height: 50px;
    font-size: 14px;
}
</style>