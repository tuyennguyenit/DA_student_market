                                        </tr>
                                        </thead>

                                        <tbody>
                                       
                                        <%if(typeof(listIDTin)!=='undefine'&& listIDTin.length>0){
                                            for(i=0;i<listIDTin.length;i++){
                                        %>
                                           
                                        <tr>
                                            <td><%=i+1%></td>
                                            <td><%=listIDTin[i].tieuDe%></td>
                                            <td><%=listIDTin[i].nguoiDang%></td>                                         
                                            <td>
                                                <a href="/chitiettd/<%=listIDTin[0].id_TinDangTheoDoi%>" class="btn-add-to-cart">Xem</a>                                              
                                                <a href="/info/delete/tindaluu/<%=listIDTin[0]._id%>" onclick="return confirm('Bạn có chắc chắn muốn xóa chứ?'); " class="btn-add-to-cart">Xóa</a>
                                            </td>
                                        </tr>
                                        <%}}%>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                                   
                                            
                                            <!--thông báo-->
                                         
