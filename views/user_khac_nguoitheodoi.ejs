<!--== Page Content Wrapper Start ==-->
<div id="page-content-wrapper" class="p-9">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <!-- My Account Page Start -->
                <div class="myaccount-page-wrapper">
                    <!-- My Account Tab Menu Start -->
                    <div class="row">
                        <div class="col-lg-3">
                            <div class="myaccount-tab-menu nav" role="tablist">
                               
                                       <form action="/info" method="post" id="form_info">
                                        <input type="hidden"name="email_User" value="<%=email_User%>">
                                         <a href="javascript:{}" onclick="document.getElementById('form_info').submit();" ><i class="fa fa-dashboard"></i>
                                            Thông Tin cá Nhân</a>
                                  </form>
                                   <form id="form_ttcn" action="/info/khac/tindadang" method="post">
                                    <input type="hidden" value="<%=user[0].email%>" name="email_User">
                                    <a href="javascript:{}"  onclick="document.getElementById('form_ttcn').submit();" ><i class="fa fa-cart-arrow-down"></i> Tin Đang Đăng</a>
                                  </form>

                                   </form>
                                   <form id="form_ttcn1" action="/user/khac/mail" method="post">
                                    <input type="hidden" value="<%=user[0].email%>" name="email_User">
                                    <input type="hidden" value="<%=user[0].id%>" name="id">
                                    <a href="javascript:{}"  onclick="document.getElementById('form_ttcn1').submit();" ><i class="fa fa-cart-arrow-down"></i> Nhắn Tin</a>
                                  </form>
                                  
                                   
    
    
    
                                    
    
                                    
                            </div>
                        </div>
                        <!-- My Account Tab Menu End -->

                        <!-- My Account Tab Content Start -->
                        <div class="col-lg-9 mt-5 mt-lg-0">
                            <div class="tab-content" id="myaccountContent">
                                <!-- Single Tab Content Start -->
                                <div class="tab-pane fade show active" id="dashboad" role="tabpanel">
                                <div class="myaccount-content">
                                <h3>Người theo dõi</h3>

                                <div class="myaccount-table table-responsive text-center">
                                    <table class="table table-bordered">
                                        <thead class="thead-light">
                                        <tr>
                                            <th>STT</th>
                                            <th>Tên</th>
                                            <th>Số Điện thoại</th>
                                           
                                        </tr>
                                        </thead>

                                        <tbody>
                                        <%if(typeof(userTheoDoi)!=='undefined' && userTheoDoi.length>0){
                                            for(i=0;i<userTheoDoi.length;i++){
                                        %>
                                        <tr>
                                            <td><%=i+1%></td>
                                            <td><%=userTheoDoi[i].name%></td>
                                            <td><%=userTheoDoi[i].sDT%></td>
                                           
                                           
                                        </tr>
                                        <%}}%>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                                   
                                            
                                            <!--thông báo-->
                                         
                                            <% if(typeof(mespass)!='undefined')
                                            { %>
                                            <br>
                                            <div class="alert alert-warning">
                                                <b>Oh snap!</b> <%=mespass%>
                                            </div>
                                            <%} %> 
                                    </div>
                                    </div>
                              
                                <!-- Single Tab Content End //end chi tiết tài khoản-->

                                
                               
                            </div>
                        </div>
                        <!-- My Account Tab Content End -->
                    </div>
                </div>
                <!-- My Account Page End -->
            </div>
        </div>
    </div>
</div>
